create_table("residents") {
    t.Column("id", "integer", {primary: true})
    t.Column("name", "string")
    t.Column("month", "string")
    t.Column("apartment_id", "integer")
}

add_foreign_key("residents", "apartment_id", {"apartments": ["id"]}, {
    "on_delete": "cascade",
    "on_update": "cascade",
})